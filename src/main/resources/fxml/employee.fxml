      <Label layoutX="103.0" layoutY="1.0" prefHeight="50.0" prefWidth="455.0" text="WELCOME TO CRIMINAL RECORDS">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <ListView fx:id="listView" layoutX="317.0" layoutY="96.0" prefHeight="233.0" prefWidth="333.0" />
      <Label layoutX="449.0" layoutY="74.0" text="All criminals">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Button fx:id="addCriminalBtn" layoutX="331.0" layoutY="339.0" mnemonicParsing="false" onAction="#showAddCriminals" text="Add new criminal" />
   </children>
