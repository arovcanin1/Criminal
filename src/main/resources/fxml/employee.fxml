<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="logoutBtn" layoutX="582.0" layoutY="13.0" mnemonicParsing="false" onAction="#showLogout" text="Logout" />

      <ImageView fitHeight="344.0" fitWidth="313.0" layoutY="62.0">
         <Image url="@../img/criminals.jpg" />
